{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:import/typescript",
        "plugin:react-js-runtime",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "@typescript-eslint/no-unused-vars": "off",
        "no-unused-vars": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        },
        "import/resolver": {
            "typescript": true
        }
    },
    "env": {
        "browser": true
    }
}
