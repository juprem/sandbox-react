{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:import/typescript",
        "eslint:recommended"
    ],
    "plugins": [
        "react-compiler"
    ],
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "@typescript-eslint/no-unused-vars": "off",
        "no-unused-vars": "off",
        "react-compiler/react-compiler": "error"
    },
    "settings": {
        "react": {
            "version": "detect"
        },
        "import/resolver": {
            "typescript": true
        }
    },
    "env": {
        "browser": true
    }
}


